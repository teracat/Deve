{
  // Application settings
  "AppSettings": {
    "JwtKeys": {
      "SigningSecretKey": "YouShouldChangeThisKey_MustBe32B", // You should change this key (must be 32 bytes)
      "EncryptionSecretKey": "YouShouldChangeThisKey_MustBe32B" // You should change this key (must be 32 bytes)
    }
  },

  // Connection strings
  "ConnectionStrings": {
    //You should set the DataSourceConnection (if needed by your DataSource)
    "DataSourceConnection": "Server=localhost;Database=dbname;User Id=user;Password=pwd;",
    //You should set the RedisCacheConnection (if using RedisCache class as the ICache implementation)
    "RedisCacheConnection": ""
  },

  // Azure Monitor settings
  "AzureMonitor": {
    // If you are using Azure Monitor, set the connection string here
    "ConnectionString": ""
  },

  // OpenTelemetry endpoint (configured in Docker compose file)
  "OTEL_EXPORTER_OTLP_ENDPOINT": "",

  // Zipkin endpoint (configured in Docker compose file)
  "ZIPKIN_URL": "",

  // Prometheus scrape endpoint
  "PROMETHEUS_SCRAPE_ENDPOINT": "/metrics",

  // Sentry
  "Sentry": {
    "Dsn": "" // You should set the Sentry Dsn (if using Sentry)
  },

  // Logging settings
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
